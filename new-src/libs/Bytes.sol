 * Lib with some byte-related methods
 */
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.18;

library BytesLib {
    /**
     * encodeBytesArray
     * Receives an array of bytes, and returns a bytes version of it. So type-casting it, basically.
     * @param bytesArr - The array of bytes to "encode"
     * @return encodedArr - An "encoded" version of the array (just casted, no additional logic)
     */
    function encodeBytesArray(
        bytes[] memory bytesArr
    ) internal pure returns (bytes memory encodedArr) {
        for (uint256 i = 0; i < bytesArr.length; i++)
            encodedArr = bytes.concat(encodedArr, bytesArr[i]);
    }
}

